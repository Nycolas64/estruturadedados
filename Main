public class Main {
    public static void main(String[] args) {
        No a = new No("A");
        No b = new No("B");
        No c = new No("C");
        No d = new No("D");
        No e = new No("E");
        No f = new No("F");

        a.esquerda = b;
        a.direita = c;
        b.esquerda = d;
        b.direita = e;
        c.esquerda = f;

        a.emOrdem();
        System.out.println();
        System.out.println(a.contarNos());
    }
}
